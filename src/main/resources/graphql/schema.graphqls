type Query {
    getEquipmentById(id: ID!): Equipment
}

type Mutation {
    createEquipment(
        name: String!
        manufacturer: String!
        description: String!
        inventoryId: ID!
    ): Boolean
    createRequirement(
        name: String!
        specialization: Specialization!
        inventoryId: ID!
    ): Boolean
    createInventory: Boolean
    deleteInventory(id: ID!): Boolean
}
